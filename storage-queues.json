	"item": [
		{
			"name": "New Request",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.request.headers.add({\r",
							"    key: \"x-ms-date\",\r",
							"    value: new Date().toUTCString()\r",
							"});\r",
							"\r",
							"function createSharedAccessToken(resourceUri, sasKeyName, sasKey) {\r",
							"    if (!resourceUri || !sasKeyName || !sasKey) {\r",
							"        throw \"Missing required parameter\";\r",
							"    }\r",
							"    const encoded = encodeURIComponent(resourceUri);\r",
							"    const now = new Date();\r",
							"    const minute = 60;\r",
							"    const ttl = Math.round(now.getTime() / 1000) + minute;\r",
							"    const signature = encoded + '\\n' + ttl;\r",
							"    const hash = CryptoJS.HmacSHA256(signature, sasKey).toString(CryptoJS.enc.Base64);\r",
							"    return 'SharedAccessSignature sr=' + encoded + '&sig=' +\r",
							"        encodeURIComponent(hash) + '&se=' + ttl + '&skn=' + sasKeyName;\r",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "PUT",
				"header": [
					{
						"key": "Authorization",
						"value": "SharedKey  name:signature",
						"type": "text"
					},
					{
						"key": "x-ms-version",
						"value": "2011-08-18",
						"type": "text"
					}
				],
